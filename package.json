{
  "name": "express-no-stress",
  "version": "0.0.1",
  "description": "A template for creating APIs Fast",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --config \"./nodemon.json\"/",
    "start": "pm2 start api/server.js",
    "author": "node author",
    "make": "node generator --name $1",
    "make:model": "node generator --model --name $1",
    "make:service": "node generator --service --name $1",
    "make:controller": "node generator --controller --name $1",
    "make:routes": "node generator --routes --name $1",
    "make:validations": "node generator --validations --name $1",
    "make:events": "node generator --events --name $1",
    "make:docs": "node generator --docs -o --name $1",
    "make:resource": "node generator --model --service --controller --routes --validations --events --docs --name $1"
  },
  "keywords": [
    "REST",
    "API"
  ],
  "author": "Alton Bell Smythe <abellsmythe@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/boxen": "^3.0.1",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/helmet": "0.0.43",
    "@types/hpp": "^0.2.1",
    "@types/mongoose": "^5.5.0",
    "@types/node": "^12.0.0",
    "@types/passport": "^1.0.0",
    "@types/swagger-jsdoc": "0.0.2",
    "@types/swagger-ui-express": "^3.0.0",
    "assert": "^1.4.1",
    "cross-env": "^1.0.8",
    "cucumber": "^1.0.0",
    "minimist": "^1.2.0",
    "nodemon": "^1.18.11",
    "request-promise": "^3.0.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "boxen": "^3.2.0",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-limiter": "^1.6.1",
    "express-session": "^1.16.1",
    "express-status-monitor": "^1.2.5",
    "express-validator": "^5.3.1",
    "helmet": "^3.16.0",
    "hpp": "^0.2.2",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pm2": "^3.5.0",
    "redis": "^2.8.0",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.2"
  }
}
